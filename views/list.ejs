<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Compromissos</title>
  </head>
  <body>
    <form action="" method="post">
      <input
        type="search"
        name="search"
        id="search"
        placeholder="Digite o compromisso"
      />
      <button>Pesquisar</button>
    </form>
    <% comms.forEach(comm => { %>
    <h3>Eventos:</h3>
    <h4>Nome: <%= comm.title %></h4>
    <h4>Email: <%= comm.email %></h4>
    <hr />
    <% }) %>
  </body>
</html>
